<%= simple_form_for(@worksheet_t) do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
  <div class="col s12">
          <center>
          <br>
          <br>
          <a class="main-green-text crete-round"><font size="6">Worksheet Results</font></a>            
          <br>
          <%=image_tag("line3.png" , height:'55')%>
          <br>
          </center>
          <br>
        </div>

  <div class="row">
    <div class="col s4 offset-s4">
      <div class="card open-sans main-white">
        <div class="card-content">
          <div class="row">
            <div class="dateinput col s12">
              <%= f.association :module_t, prompt: 'Module', collection: ModuleT.order('file_file_name ASC'), label: 'Module' %>
            </div>
          </div>

          <div class="row">
            <div class="dateinput col s12">
              <%= f.association :tutee_t, prompt: 'Tutee', collection: TuteeT.order('last_name ASC'), label: 'Tutee' %>
            </div>              
          </div>
          <div class="row">
            <div class="dateinput col s6">
              <%= f.input :score %>
            </div>

             <div class="dateinput col s6">
              <%= f.input :reached_quota, label: 'Reached Quota?', collection: ['Yes', 'No'] %>
            </div>
          </div> 
       <div class="row">
            <div class="col s12">
<center>
                <%= f.button :submit, "Submit", :class => 'waves-effect waves-light btn btn-main-green' %>
                <%= link_to 'Cancel', worksheet_ts_path, :class => 'waves-effect waves-light btn btn-main-green' %>
              </center>
            </div>                 
          </div>  

          <% end %>
        </div>
      </div>
    </div>
  </div>
